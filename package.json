{
  "name": "CalBot",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --remote",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "test": "vitest run",
    "test:watch": "vitest",
    "vectorize:create": "wrangler vectorize create nutrition-foods --dimensions=1024 --metric=cosine",
    "eval": "tsx evals/run.ts",
    "eval:ci": "EVAL_USE_MOCK=1 tsx evals/run.ts"
  },
  "dependencies": {
    "hono": "^4.11.7",
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@ikrigel/ragas-lib-typescript": "^0.1.1",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.4.0"
  }
}